{
  "manifest_version": 3,
  "name": "pyてょん to python",
  "short_name": "pyてょん to python",
  "version": "2.1.0",
  "description": "「pyてょん」と誤変換したものを変換確定後に「python」自動変換します",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "src/popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-DcM-za5v.js"
      ],
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/globalFunctions-CMZ0Dx9V.js",
        "assets/globalFunctions-CMZ0Dx9V.js.map",
        "assets/content.ts-BuqaKyX3.js",
        "assets/content.ts-BuqaKyX3.js.map"
      ],
      "use_dynamic_url": false
    }
  ]
}
