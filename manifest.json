{
  "manifest_version": 3,
  "name": "pyてょん to python",
  "short_name":"pyてょん to python",
  "version": "1.0",
  "description": "「「pyてょん」と誤変換したものを変換確定後に「python」自動変換します",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}